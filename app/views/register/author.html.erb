<div class="bs-example">
    <h1>AUTHOR Sign Up</h1>
    <form class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-xs-3" for="inputEmail">Email:</label>
            <div class="col-xs-9">
                <input type="email" class="form-control tip-top" id="inputEmail" placeholder="Email" data-placement="top" data-toggle="tooltip" data-original-title="What's your email address?">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3" for="inputPassword">Password:</label>
            <div class="col-xs-9">
                <input type="password" class="form-control tip-top" id="inputPassword" placeholder="Password" data-placement="top" data-toggle="tooltip" data-original-title="password">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3" for="confirmPassword">Confirm Password:</label>
            <div class="col-xs-9">
                <input type="password" class="form-control tip-top" id="confirmPassword" placeholder="Confirm Password"data-placement="top" data-toggle="tooltip" data-original-title="type same password">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3" for="firstName">First Name:</label>
            <div class="col-xs-9">
                <input type="text" class="form-control tip-top" id="firstName" placeholder="First Name" data-placement="top" data-toggle="tooltip" data-original-title="What's your first name?">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3" for="lastName">Last Name:</label>
            <div class="col-xs-9">
                <input type="text" class="form-control tip-top" id="lastName" placeholder="Last Name"data-placement="top" data-toggle="tooltip" data-original-title="What's your last name?">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3" for="phoneNumber">Phone:</label>
            <div class="col-xs-9">
                <input type="tel" class="form-control tip-top" id="phoneNumber" placeholder="Phone Number"data-placement="top" data-toggle="tooltip" data-original-title="What's your contact number?">
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3">Date of Birth:</label>
            <div class="col-xs-3">
                <select class="form-control">
                    <option>Date</option>
                </select>
            </div>
            <div class="col-xs-3">
                <select class="form-control">
                    <option>Month</option>
                </select>
            </div>
            <div class="col-xs-3">
                <select class="form-control">
                    <option>Year</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3" for="postalAddress">Address:</label>
            <div class="col-xs-9">
                <textarea rows="3" class="form-control tip-top" id="postalAddress" placeholder="Postal Address"data-placement="top" data-toggle="tooltip" data-original-title="What's your address?"></textarea>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-xs-3" for="ZipCode">Zip Code:</label>
            <div class="col-xs-9">
                <input type="number" class="form-control tip-top" id="ZipCode" placeholder="Zip Code"data-placement="top" data-toggle="tooltip" data-original-title="What's your zip code?">
            </div>
        </div>
		<div class="form-group">
            <label class="control-label col-xs-3" for="ZipCode">Aadhar card number:</label>
            <div class="col-xs-9">
                <input type="number" class="form-control tip-top" id="ZipCode" placeholder="Aadhar Number"data-placement="top" data-toggle="tooltip" data-original-title="enter vaild aadhar card number">
            </div>
        </div>	
        <div class="form-group">
            <label class="control-label col-xs-3">Gender:</label>
            <div class="col-xs-2">
                <label class="radio-inline">
                    <input type="radio" name="genderRadios" value="male"> Male
                </label>
            </div>
            <div class="col-xs-2">
                <label class="radio-inline">
                    <input type="radio" name="genderRadios" value="female"> Female
                </label>
            </div>
        </div>

        <div class="form-group">
            <div class="col-xs-offset-3 col-xs-9">
                <label class="checkbox-inline">
                    <input type="checkbox" value="agree">  I agree to the <a href="#">Terms and Conditions</a>.
                </label>
            </div>
        </div>
        <br>
        <div class="form-group">
            <div class="col-xs-offset-3 col-xs-9">
                <input type="submit" class="btn btn-primary" value="Submit">
                <input type="reset" class="btn btn-default" value="Reset">
            </div>
        </div>
    </form>
