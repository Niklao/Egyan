<h1>AUTHOR Sign Up</h1>
  	<div class="container">
	  	<%= form_for @user, url: {action: "entered"}, html: {class: "form_horizontal"} do |f| %>
		  <%= f.label(:email_id , "Email Id" ,class: "control-label col-xs-3")  %>
		  <%= f.text_field :email_id , class: "form-control tip-top"%>
		  <%= f.label :first_name , class: "control-label col-xs-3" , placeholder: "Email" %>
		  <%= f.text_field :first_name , class: "form-control tip-top" %>
		  <%= f.label :last_name , class: "control-label col-xs-3" %>
		  <%= f.text_field :last_name , class: "form-control tip-top" %>
		  <%= f.label :password , class: "control-label col-xs-3" %>
		  <%= f.password_field :password  , class: "form-control tip-top"  %>
		  <%= f.label(:password ,  "Confirm Password" , class: "control-label col-xs-3" ) %>
		  <%= f.password_field :password  , class: "form-control tip-top"  %>
		  <%= f.label(:birth_date ,  "Birth Date" , class: "control-label col-xs-3" ) %>
		  <%= f.date_select( :birth_date ,class: "form-control" )%>
		  <%= f.label(:address ,  "Address" , class: "control-label col-xs-3" ) %>
		  <%= f.text_area( :address ,class: "form-control" )%>
		  <%= f.submit "Create" , class: "btn btn-primary"%>
		  
		<% end %>
	</div>
